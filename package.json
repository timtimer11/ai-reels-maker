{
  "name": "nextjs-fastapi",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload --reload-exclude='api/services/*' --reload-exclude='api/routers/*' --timeout-keep-alive 75",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.22.0",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.2",
    "@types/node": "22.5.5",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@upstash/ratelimit": "^2.0.5",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "geist": "^1.4.2",
    "lucide-react": "^0.509.0",
    "next": "^14.2.13",
    "postcss": "^8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "3.4.12",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.6.2"
  }
}
